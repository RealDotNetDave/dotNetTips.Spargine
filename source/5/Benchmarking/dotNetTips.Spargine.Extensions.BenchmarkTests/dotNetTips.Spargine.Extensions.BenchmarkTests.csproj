		<PackageReference Include="dotNetTips.Spargine.5.Core" Version="2021.6.3.5" />
		<PackageReference Include="dotNetTips.Spargine.5.Extensions" Version="2021.6.3.5" />
		<PackageReference Include="dotNetTips.Spargine.5.Tester" Version="2021.6.3.5" />
		<PackageReference Include="Microsoft.Data.Services.Client" Version="5.8.4" />
		<PackageReference Include="System.ServiceModel.Primitives" Version="4.8.1" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\dotNetTips.Spargine.5.Benchmarking\dotNetTips.Spargine.5.Benchmarking.csproj" />
	</ItemGroup>

</Project>
